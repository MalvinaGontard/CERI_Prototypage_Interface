<?xml version="1.0" encoding="UTF-8"?>

<?import java.net.URL?>
<?import javafx.geometry.Insets?>
<?import javafx.scene.Cursor?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.control.ToolBar?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.HBox?>
<?import javafx.scene.layout.Pane?>

<AnchorPane id="AnchorPane" prefHeight="0.0" prefWidth="600.0" styleClass="mainFxmlClass" xmlns:fx="http://javafx.com/fxml/1" xmlns="http://javafx.com/javafx/8.0.171" fx:controller="javafxapplication3.FXMLToolBarController">
    <stylesheets>
        <URL value="@fxmltoolbar.css" />
    </stylesheets>
   <children>
      <ToolBar layoutX="10.0" layoutY="10.0" prefHeight="54.0" prefWidth="650.0" AnchorPane.bottomAnchor="0.0" AnchorPane.leftAnchor="0.0" AnchorPane.rightAnchor="0.0" AnchorPane.topAnchor="0.0">
         <items>
            <Button fx:id="addContact" mnemonicParsing="false" text="Ajouter" />
            <Button mnemonicParsing="false" text="Modifier" />
            <Button mnemonicParsing="false" text="Supprimer" />
            <Pane HBox.hgrow="ALWAYS">
               <children>
                  <HBox />
               </children>
            </Pane>
            <TextField alignment="CENTER_RIGHT" promptText="Rechercher" />
            <Button id="search" alignment="CENTER_RIGHT" contentDisplay="RIGHT" mnemonicParsing="false" text="Rechercher">
               <cursor>
                  <Cursor fx:constant="OPEN_HAND" />
               </cursor>
            </Button>
         </items>
         <opaqueInsets>
            <Insets />
         </opaqueInsets>
      </ToolBar>
   </children>
</AnchorPane>
